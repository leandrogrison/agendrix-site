{
  "buildCommand": "npm run build:ssr && npm run vercel-build",
  "outputDirectory": ".vercel/output",
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "dist/browser/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.js" }
  ]
}
